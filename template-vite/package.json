{
    "name": "msc-down",
    "version": "1.0.0",
    "description": "A project template by electron & vite & vue3",
    "main": "./app/main.js",
    "scripts": {
        "dev": "node ./build/development.js",
        "build": "node ./build/build.js",
        "build1": "npm run build:vite && npm run build:electron",
        "dev:vite": "vite",
        "dev:electron": "webpack-cli --config webpack.config.js --progress --mode=development && electron .",
        "build:vite": "vite build --base=./",
        "build:electron": "webpack-cli --config webpack.config.js --progress --mode=production",
        "package": "npm run build && electron-builder --config electron-builder-config.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/guolaopi/vite-electron-template.git"
    },
    "keywords": [
        "electron",
        "es6",
        "template"
    ],
    "author": "guolaopi",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/guolaopi/vite-electron-template/issues"
    },
    "homepage": "https://github.com/guolaopi/vite-electron-template#readme",
    "devDependencies": {
        "@electron/remote": "^2.0.8",
        "@vue/compiler-sfc": "^3.0.4",
        "babel-core": "^6.25.0",
        "babel-loader": "^7.1.1",
        "babel-preset-es2015": "^6.24.1",
        "child_process": "^1.0.2",
        "electron": "^18.2.4",
        "electron-builder": "^23.1.0",
        "vite": "^1.0.0-rc.13",
        "vue": "^3.0.4",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.10.0"
    }
}
